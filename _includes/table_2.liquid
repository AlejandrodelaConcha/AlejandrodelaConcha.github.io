<div class="list-groups">
  {% for content in data[1] %}
    <div class="list-group col-md-6">
      <table class="table table-sm table-borderless">
        <tbody>
          {% for item in content.keywords %}
            <tr>
              <td></td>
              <td class="list-group-name">
                <b>{{ item }}</b>
              </td>
            </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>
  {% endfor %}
</div>